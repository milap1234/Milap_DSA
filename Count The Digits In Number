public class countTheDigits{
 public static void main(String[] args) {
System.out.println("Enter number ");
                  Scanner mg = new Scanner(System.in);
                int  n=  mg.nextInt();
                int count =0 ;
                while(n!=0)
                {
                    n = n/10;
                    count++;
                }
                System.out.println(count);
}
}
